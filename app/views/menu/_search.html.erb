<li role="search">
  <%= form_tag(search_users_path, :method => :get) do %>
    <div class="form-group form-group-search">
      <%= text_field_tag :users_search, params[:users_search], :autocomplete => "off", :data => {:provide => "typeahead"}, :id => "search", :placeholder => t("search.search_users") %>
      <span class="glyphicon glyphicon-search"></span>
    </div>
  <% end %>
</li>
